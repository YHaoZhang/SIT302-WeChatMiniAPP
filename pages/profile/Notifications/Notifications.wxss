/* pages/profile/Notifications/Notifications.wxss */
.back_img{
  width: 100%;
  float: left;
  position: fixed;
}
.content .cell{
  width: 100%;
  float: left;
  margin-bottom: 60rpx;
  background-color: rgba(118, 198, 158, 0.6);
  border-radius: 20rpx;
}
.content .cell image{
  width: 36%;
  margin-left: 20rpx;
  margin-top: 20rpx;
  float: left;
  border-radius: 20rpx;
}
.content .cell .box{
  float: left;
}
.content .cell .box .tit{
  display:block;
  font-size: 40rpx;
  color: black;
  font-weight: bold;
  margin-top: 50rpx;
  margin-left: 20rpx;
}
.content .cell .box .info{
  display:block;
  color: rgba(36, 33, 38, 0.5);
  font-weight: normal;
  margin-top: 10rpx;
  margin-left: 20rpx;
}
.content .detail-txt{
  width: 100%;
  float: left;
  margin-left: 20rpx;
}
.content .detail-txt .box{
  width: 100%;
  float: left;
}
.content .detail-txt .box .name{
  width: 100%;
  float: left;
  font-size: 30rpx;
  font-weight: bold
}
.content .detail-txt .box .timer{
  width: 100%;
  float: left;
  font-size: 30rpx;
  color: #707070;
}
.noEvent{
  margin-top: 70%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  font-weight: bold;
  font-size: 35rpx;
}