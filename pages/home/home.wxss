/* pages/home/home.wxss */
.banner{
  width: 100%;
  height: 300rpx;
  background:white;
  float: left;
}
.banner swiper{
  height: 100%;
  width: 100%;
  float: left;
}
.banner swiper image{
  width: 100%;
  height: 100%;
  float: left
}
.main-box,
.coop-box {
  width: 100%;
  float: left
}
.main-box text,
.coop-box text {
  font-size: 48rpx;
  font-weight:bold;
  color: #5abd8c;
  text-align: left;
  margin-left: 40rpx;
  padding-top: 40rpx;
  padding-bottom: 20rpx;
  display:block;
  letter-spacing: 1rpx;
}
.main-box image,
.coop-box image {
  width: 100%;
  float: left;
}